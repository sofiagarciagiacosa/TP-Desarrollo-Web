<!DOCTYPE html>
<html>
    <head>
        <title>AURA online shop</title>
        <link rel="stylesheet" href="../css/headerNuevo.css" />
        <link rel="stylesheet" href="../css/footer.css" />
        <link rel="stylesheet" href="../css/productList.css" />
        
    </head>
    <body>
        <%- include("../partials/header.ejs") %>
        <main>
            <section class="products">
                <div class="titulo"><h2>living</h2></div>
                <div class="productsCaja">
                    <ul>
                    <% productsLiving.forEach(function (product) { %>
                        <div class="caja">
                            <li>
                                <a href="/living/<%= product.id %>">
                                    <img src=<%= product.image %> />
                                    <div class="texto1"><h3><%= product.name %> </h3></div>
                                    <div class="texto2"><p><%= product.price %> </p></div>
                                    <div class="texto3"><p><%= product.description %></p></div>
                                </a>
                            </li>
                        </div>
                    
                    <% }); %>
                </ul>
                </div>
                
            </section>

        </main>
        <%- include("../partials/footer.ejs") %>
    </body>
</html>